{
  "name": "airframe-dashboard",
  "version": "0.1.0",
  "description": "Seed project for flexible light React/Bootstrap 4 dashboards.",
  "license": "ISC",
  "author": "Webkom",
  "main": "server.js",
  "scripts": {
    "start": "npm run start:prod",
    "build": "npm run build:prod",
    "clean": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 ./build/cli-tools.js --clear dist --create dist",
    "test": "mocha --exit 'backend/test'",
    "test-clean": "npm run init-import && npm run test",
    "import-examples": "node backend/db/importData.js",
    "init-db": "node backend/db/dropTables.js && node backend/db/initDb.js",
    "init-import": "npm run init-db && npm run import-examples",
    "heroku-postbuild": "npm run build:prod",
    "build:dev": "npm run clean && webpack --config ./build/webpack.config.client.dev.js",
    "build:prod": "npm run clean && node --optimize_for_size --max_old_space_size=460 --gc_interval=100 ./node_modules/webpack/bin/webpack.js -p --progress --config ./build/webpack.config.client.prod.js",
    "start:dev": "npm run build:dev && node index.js",
    "start:dev-hot": "npm run clean && webpack-dev-server --config ./build/webpack.config.client.dev.js",
    "start:prod": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 index.js"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "mocha": "^7.1.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@owczar/dashboard-style--airframe": "^0.1.13",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-universal-import": "^4.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "circular-dependency-plugin": "^5.0.2",
    "commander": "^2.20.0",
    "css-loader": "^3.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "extract-css-chunks-webpack-plugin": "^4.5.2",
    "file-loader": "^4.0.0",
    "font-awesome": "^4.7.0",
    "holderjs": "^2.9.6",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pg": "^8.0.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.7.0",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-bootstrap-table-next": "^3.1.4",
    "react-bootstrap-table2-editor": "^1.2.4",
    "react-bootstrap-table2-filter": "^1.1.9",
    "react-bootstrap-table2-paginator": "^2.0.6",
    "react-bootstrap-table2-toolkit": "^2.0.1",
    "react-dom": "^16.8.6",
    "react-grid-layout": "^0.16.6",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.11.0",
    "react-responsive": "^7.0.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sequelize": "^5.21.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "uuid": "^3.3.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.7.1"
  },
  "proxy": "http://localhost:3001"
}
